Easy 1
def length_of_last_word(s):
  """
  Returns the length of the last word in the string.
  """
  i = len(s) - 1
  while i >= 0 and s[i] == " ":
    i -= 1
  j = i
  while i >= 0 and s[i] != " ":
    i -= 1
  return j - i

# Example usage
s = "Hello World"
print(length_of_last_word(s))

s = " fly me to the moon "
print(length_of_last_word(s))

s = "luffy is still joyboy"
print(length_of_last_word(s))

output is 
5
4
6
